<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vyw.rephotoandroid">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:requestLegacyExternalStorage="true">
        <activity
            android:name="com.vyw.rephotoandroid.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.vyw.rephotoandroid.CameraActivity" />
        <activity android:name="com.vyw.rephotoandroid.NavigationProcesing" />
        <activity android:name="com.vyw.rephotoandroid.SelectPointsActivity" />
        <activity android:name="com.vyw.rephotoandroid.RefotografieActivity" />
        <activity android:name="com.vyw.rephotoandroid.SettingActivity" />
    </application>

</manifest>